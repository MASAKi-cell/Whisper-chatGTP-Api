{
  "cSpell.words": [
    "buildx",
    "callout",
    "dotenv",
    "openai",
    "stylelint",
    "vercel",
    "VOICEVOX"
  ],
  "files.exclude": {
    "**/node_modules": true,
    "**/venv": true,
    "**/.next": true,
  },
  "editor.formatOnType": true,
  "editor.defaultFormatter": "ms-python.black-formatter",
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  }
}
